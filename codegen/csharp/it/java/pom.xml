    <profile>
      <id>jcc8</id>
      <activation>
        <file>
          <exists>${basedir}/src/main/javacc</exists>
        </file>
      </activation>
      <build>
        <pluginManagement>
          <plugins>
            <plugin>
              <groupId>org.javacc.plugin</groupId>
              <artifactId>javacc-maven-plugin</artifactId>
              <dependencies>
                <dependency>
                  <groupId>org.javacc</groupId>
                  <artifactId>core</artifactId>
                  <version>${project.version}</version>
                  <scope>runtime</scope>
                </dependency>
                <dependency>
                  <groupId>org.javacc.codegen</groupId>
                  <artifactId>java</artifactId>
                  <version>${project.version}</version>
                  <scope>runtime</scope>
                </dependency>
              </dependencies>
            </plugin>
          </plugins>
        </pluginManagement>
        <plugins>
          <plugin>
            <groupId>org.javacc.plugin</groupId>
            <artifactId>javacc-maven-plugin</artifactId>
            <executions>
              <execution>
                <id>jcc8jcc</id>
                <phase>generate-sources</phase>
                <goals>
                  <goal>javacc</goal>
                </goals>
                <configuration>
                  <outputDirectory>${project.basedir}/target/generated-sources/java</outputDirectory>
                  <sourceDirectory>${project.basedir}/src/main/javacc</sourceDirectory>
                  <codeGenerator>java</codeGenerator>
                </configuration>
              </execution>
            </executions>
          </plugin>
        </plugins>
      </build>
    </profile>
